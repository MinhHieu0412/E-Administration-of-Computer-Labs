<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
    <!-- Favicon Link -->
    <link rel="icon" href="assests/img/favicon.ico">
    
    <!-- External CSS -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">
    <link rel="stylesheet" href=@Url.Content("~/fontawesome/css/all.min.css")>

    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/responsive.css">`

</head>
<body>
    <div id="dashboard-section">
        <div class="row no-gutters">
            <div class="col-lg-2 col-md-3 col-12 sidebar-nav" style="background: #f1f1f1;">
                <!-- Sidebar Section -->
                    @await Html.PartialAsync("_Sidebar")
                <!-- !Sidebar Section -->
            </div>
            <div class="col-lg-10 col-md-9 col-12 content-data">
                <!-- Header Section -->
                    @await Html.PartialAsync("_Header")
                <!-- !Header Section -->
                <!-- Main Dashboard Section -->
                @RenderBody()
                <!-- !Main Dashboard Section -->
                <!-- Footer Section -->
                @await Html.PartialAsync("_Footer")
                <!-- Footer Section -->
            </div>
        </div>

    </div>

    <!-- External JS -->
    <script src="@Url.Content("~/js/jquery-3.5.1.min.js")"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.1/umd/popper.min.js"></script>

    <script src="@Url.Content("~/js/bootstrap.min.js")"></script>

    <script src="~/js/owl.carousel.min.js"></script>
    <!-- Custom JS -->
    <script src="~/js/script.js"></script>

</body>
</html>